uci set passwall.Speed.domain_list='speed-c2201-rule'
uci set passwall.Twitter=shunt_rules
uci set passwall.Twitter.remarks='Twitter'
uci set passwall.Twitter.domain_list='geosite:twitter'
uci set passwall.Direct=shunt_rules
uci set passwall.Direct.remarks='Direct'
uci set passwall.Direct.domain_list='geosite:cn
geosite:apple-cn
