uci set passwall.myshunt=nodes
uci set passwall.myshunt.remarks='分流总节点'
uci set passwall.myshunt.type='Xray'
uci set passwall.myshunt.protocol='_shunt'
uci set passwall.myshunt.BT='_direct'
